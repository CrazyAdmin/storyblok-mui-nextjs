{
  "version": 2,
  "name": "next-demo-material",
  "alias": "com2.lumen.media",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "^/(favicon.ico|robots.txt)$",
      "headers": {
        "Cache-Control": "public, s-maxage=31536000, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "^/_next/static/(.*)$",
      "headers": {
        "Cache-Control": "public, s-maxage=31536000, max-age=31536000, immutable"
      },
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "cache-control": "s-maxage=60"
      }
    }
  ]
}
